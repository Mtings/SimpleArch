<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ForegroundImageView">
        <attr name="android:foreground" />
    </declare-styleable>

    <declare-styleable name="FlashTextView">
        <attr name="corner_float" format="dimension" />
        <attr name="text" format="string" />
        <attr name="textType">
            <!--字母-->
            <enum name="letter" value="0x01" />
            <!--数字-->
            <enum name="number" value="0x02" />
            <!--符号-->
            <enum name="symbol" value="0x03" />
            <!--中文-->
            <enum name="chinese" value="0x04" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FlashFrameView">
        <attr name="child_text" format="string" />
        <attr name="child_textType">
            <!--字母-->
            <enum name="letter" value="0x01" />
            <!--数字-->
            <enum name="number" value="0x02" />
            <!--符号-->
            <enum name="symbol" value="0x03" />
            <!--中文-->
            <enum name="chinese" value="0x04" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AlignedTextView">
        <attr name="alignment">
            <!--上面-->
            <enum name="top" value="0x01" />
            <!--下面-->
            <enum name="bottom" value="0x02" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CountDownText">
        <attr name="resetSymbol" format="string" />

        <attr name="digitDividerColor" format="color" />
        <attr name="digitTopDrawable" format="integer" />
        <attr name="digitBottomDrawable" format="integer" />
        <attr name="digitTextColor" format="color" />
        <attr name="halfDigitHeight" format="dimension" />
        <attr name="digitWidth" format="dimension" />
        <attr name="digitTextSize" format="dimension" />
        <attr name="digitPadding" format="dimension" />

        <attr name="animationDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="DialogFragmentNavigator">
        <attr name="android:name" />
    </declare-styleable>

    <declare-styleable name="FragmentNavigator">
        <attr name="android:name" />
    </declare-styleable>

    <declare-styleable name="NavHostFragment">
        <attr name="defaultNavHost" format="boolean" />
    </declare-styleable>

</resources>